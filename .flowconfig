[options]
suppress_comment= \\(.\\|\n\\)*\\#FlowExpectError
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable

module.name_mapper='.*\(.mstyl\|.styl\|.css\|.png\|.woff\|.woff2\|.ttf\|.eot\|.svg\)' -> 'empty/object'

[ignore]
<PROJECT_ROOT>/bin/.*
<PROJECT_ROOT>/devBundle/.*

[libs]
flow-typed
